    <div class="col-xs-12 col-sm-6 col-md-4">
        <h2>{{day}} Classes</h2>
        <h5>...</h5>

        <div class="panel-group" id="accordion">
        <!-- Class Detail Accordians -->
                    {{#each data}}
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#timetable-id-{{_id}}">
                                <i class="glyphicon glyphicon-ok-circle"></i> {{name}} ({{classes.start}})</a>
                        </div>
                        <div id="timetable-id-{{_id}}" class="panel-collapse collapse">
                            <div class="panel-body">
                                <address>
                                    <strong>({{classes.start}} - {{classes.finish}})</strong><br />{{address.building}}, {{address.street}}, {{address.town}}<br />{{address.postcode}}
                                </address>
                                <img class="street_view" height="300px" width="300px" src="{{../cfg.url}}{{../cfg.path}}/public/img/gkr-logo.png" data-latlon="{{{ geopos address }}}" data-heading="" />
                                <div class="travel-info"></div>
                            </div>
                        </div>
                    </div>
                    {{/each}}

                    <input name="class_data" id="class_data" type="hidden" value='{{{ json data }}}' />
            <!-- /Class Detail Accordians -->
        </div>

        <!-- Weekday buttons -->
        <div id="pickday" class="btn-group btn-group" role="group">
            <button type="button" class="btn btn-primary" id="btnMonday" data-value="Monday">Mon</button>
            <button type="button" class="btn btn-primary" id="btnTuesday" data-value="Tuesday">Tue</button>
            <button type="button" class="btn btn-primary" id="btnWednesday" data-value="Wednesday">Wed</button>
            <button type="button" class="btn btn-primary" id="btnThursday" data-value="Thursday">Thu</button>
            <button type="button" class="btn btn-primary" id="btnFriday" data-value="Friday">Fri</button>
            <button type="button" class="btn btn-primary" id="btnSaturday" data-value="Saturday">Sat</button>
            <button type="button" class="btn btn-primary" id="btnSunday" data-value="Sunday">Sun</button>
        </div>
        <!-- /Weekday buttons -->


    </div>
    <div class="col-sm-5 col-md-7 col-lg-7 col-md-offset-1 visible-sm visible-md visible-lg">
        <h3>Map</h3>
        <h5 style="margin-top: -10px;">Click on GKR icons to expand class info</h5>
        <div id="map_canvas" style="width:400px; height:400px;">Unfortunately we can't get your location from your browser, either because it doesn't support it or you've disabled that option.</div>
    </div>